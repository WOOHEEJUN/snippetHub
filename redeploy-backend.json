{
    "DocumentName": "AWS-RunShellScript",
    "Parameters": {
        "commands": [
            "pkill -f java",
            "sleep 5",
            "cd ~/app",
            "aws s3 cp s3://snippethub/api-0.0.1-SNAPSHOT.jar .",
            "nohup java -jar api-0.0.1-SNAPSHOT.jar > app.log 2>&1 &",
            "sleep 30",
            "curl http://localhost:8080/api/health"
        ]
    }
}

